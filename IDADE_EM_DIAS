def conversao(idade): # CRIANDO UMA FUNÇÃO E PASSANDO O ARGUMENTO IDADE.
  anos = idade // 365 # CRIANDO UMA VARIÁVEL ANOS E ATRIBUINDO A ELA O RESULTADO DA DIVISÃO DA IDADE POR 365.
  meses = (idade % 365) // 30 # CRIANDO UMA VARIÁVEL MESES E ATRIBUINDO A ELA O RESULTADO DA DIVISÃO DA IDADE POR 365 E O RESTO DA DIVISÃO.
  dias = (idade % 365) % 30 # CRIANDO UMA VARIÁVEL DIAS E ATRIBUINDO A ELA O RESTO DA DIVISÃO DA IDADE POR 365 E O RESTO DA DIVISÃO.
  return anos, meses,dias # RETORNANDO AS VARIÁVEIS ANOS, MESES E DIAS.

idade = int(input()) # CRIANDO UMA VARIÁVEL IDADE E ATRIBUINDO A ELA O VALOR
ano, mes, dia = conversao(idade) # CRIANDO UMA VARIÁVEL ANO, MES E DIA E CHAMANDO A FUNÇÃO CONVERSÃO E PASSANDO A VARIÁVEL IDADE.
# IMPRIMINDO O RESULTADO.
print(f"{ano:}",'ano (s)')
print(f"{mes:}",'mes (es)')
print(f"{dia:}",'dia (s)')
#FIM DO CODIGO
